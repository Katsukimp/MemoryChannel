{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "MemoryEventBus": "Debug",
      "MemoryEventBus.Infrastructure.Events": "Information"
    },
    "Console": {
      "FormatterName": "custom",
      "FormatterOptions": {
        "TimestampFormat": "[yyyy-MM-dd HH:mm:ss.fff] "
      }
    }
  },
  "EventBus": {
    "UseEnhancedFeatures": true
  }
}